{
	"name": "@layerd/ui",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit sync",
		"dev:build": "svelte-package -i src/lib --watch || pnpm dev:build",
		"build": "svelte-kit sync && pnpm barrel && pnpm components && pnpm styles",
		"styles": "postcss ./src/lib/ui.css -o ./dist/ui.css",
		"barrel": "barrels",
		"components": "svelte-package -i src/lib",
		"sync": "svelte-kit sync",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "pnpm build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"files": [
		"dist",
		"src/lib",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"!src/**/*.test.*",
		"!src/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"style": "./dist/ui.css",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"style": "./dist/ui.css",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./src": {
			"types": "./src/lib/index.ts",
			"svelte": "./src/lib/index.ts",
			"import": "./src/lib/index.ts",
			"default": "./src/lib/index.ts"
		},
		"./package.json": "./package.json",
		"./ui.css": "./dist/ui.css"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@csstools/postcss-light-dark-function": "^2.0.11",
		"@iconify/tailwind4": "^1.0.6",
		"@layerd/config-svelte": "workspace:*",
		"@layerd/config-ts": "workspace:*",
		"@layerd/config-vite": "workspace:*",
		"@layerd/tools": "workspace:*",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.1",
		"@tailwindcss/cli": "4.1.13",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "4.1.13",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "4.1.13",
		"@types/three": "^0.180.0",
		"postcss": "^8.5.6",
		"postcss-cli": "^11.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"publint": "^0.3.13",
		"svelte": "^5.39.8",
		"svelte-check": "^4.3.2",
		"tailwindcss": "4.1.13",
		"typescript": "^5.9.3",
		"vite": "^7.1.9",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@fontsource-variable/asta-sans": "^5.2.4",
		"@fontsource-variable/inter": "^5.2.8",
		"@fontsource-variable/jetbrains-mono": "^5.2.8",
		"@fontsource/inter": "^5.2.8",
		"@iconify-json/carbon": "^1.2.13",
		"@iconify-json/devicon": "^1.2.45",
		"@iconify-json/heroicons": "^1.2.3",
		"@iconify-json/logos": "^1.2.9",
		"@iconify-json/material-symbols": "^1.2.40",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify-json/solar": "^1.2.4",
		"@iconify-json/streamline-logos": "^1.2.0",
		"@iconify-json/svg-spinners": "^1.2.4",
		"@sveltejs/kit": "^2.43.8",
		"embla-carousel": "^8.6.0",
		"embla-carousel-auto-scroll": "^8.6.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-class-names": "^8.6.0",
		"embla-carousel-svelte": "^8.6.0",
		"globe.gl": "^2.44.0",
		"mode-watcher": "^1.1.0",
		"runed": "^0.34.0",
		"three": "^0.180.0"
	},
	"browserslist": [
		">0.5%",
		"last 2 versions",
		"iOS >= 12"
	]
}